{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:fast": "cross-env NODE_OPTIONS='--max-old-space-size=4096' next dev --turbo",
    "dev:optimized": "cross-env NODE_OPTIONS='--max-old-space-size=4096 --no-deprecation' next dev --turbo",
    "dev:test": "cross-env NODE_ENV=test next dev -p 3000",
    "build": "cross-env NODE_ENV=production TURBOPACK=0 next build",
    "build:prod": "cross-env NODE_ENV=production next build",
    "build:legacy": "node scripts/build-production.js",
    "build:test": "cross-env NODE_ENV=test next build",
    "build:optimized": "node scripts/optimize-build.js",
    "build:fast": "node scripts/optimize-build.js --frontend-only",
    "build:clean": "node scripts/optimize-build.js --clean",
    "start": "next start",
    "start:prod": "cross-env NODE_ENV=production next start -p 3000",
    "start:test": "cross-env NODE_ENV=test next start -p 3000",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "analyze": "cross-env ANALYZE=true next build",
    "export": "next export",
    "pre-commit": "node scripts/pre-commit-check.js",
    "check-imports": "node scripts/pre-commit-check.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test:types": "tsc --noEmit",
    "test:lint": "eslint . --ext .ts,.tsx",
    "test:all": "npm run test:types && npm run test:lint",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:business": "node run-business-test.js",
    "test:business:debug": "npx playwright test tests/e2e/business-flow/real-business-test.spec.js --headed --debug",
    "test:check-env": "node check-test-env.js",
    "ws:server": "node scripts/websocket-server.js",
    "dev:with-ws": "concurrently \"npm run ws:server\" \"npm run dev\""
  },
